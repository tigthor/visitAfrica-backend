{
  "name": "visitafrica-backend",
  "version": "1.0.0",
  "description": "This is the visit africa backend",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./src/tests/main.test.js --timeout 100000 --exit",
    "startDev": "nodemon --exec babel-node src/server.js",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "lint:fixIssues": "./node_modules/.bin/eslint . --fix",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "NODE_ENV=production npm run db-migrate && NODE_ENV=production babel-node ./src/server.js",
    "db-migrate": "npx sequelize db:migrate"
  },
  "repository": {
    "type": "git",
